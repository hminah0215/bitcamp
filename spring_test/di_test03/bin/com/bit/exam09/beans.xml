<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="SMSsender" class="com.bit.exam09.SMSsender">
		<property name="from" value="홍길동"/>
		<property name="to" value="이순신"/>
	</bean>
	<bean id="sm" class="com.bit.exam09.SystemMonitor"
		autowire="byType" init-method="monitor">
		<property name="periodTime" value="30"/>
	</bean>
	
</beans>
